<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.nwsuaf.gov.model">
	<class name="Train_Resource" table="train_Resources">
		<id name="resources_id">
	<generator class="native"/>
	
</id>
		<property name="resources_title" column="resources_title" />
		<property name="resources_time" column="resources_time" />
		<property name="resources_colum" column="resources_colum" />
		<property name="resources_from" column="resources_from" />
		<property name="resources_author" column="resources_author" />
		<property name="resources_profile" column="resources_profile" />
		<property name="resources_path" column="resources_path" />
	</class>
</hibernate-mapping>